<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF">

    <android.support.v7.widget.CardView
        android:id="@+id/cardViewFixture"
        android:layout_width="fill_parent"
        android:layout_height="100dp"
        android:layout_gravity="fill_horizontal"
        android:backgroundTint="#FFFFFF"
        android:layout_margin="2dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">

        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/textViewDate"
                android:layout_width="match_parent"
                android:layout_height="35dp"

                android:layout_alignEnd="@+id/textViewDay"
                android:layout_alignLeft="@+id/textViewDay"
                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/textViewDay"
                android:layout_alignStart="@+id/textViewDay"
                android:gravity="center"
                android:text="Friends"
                android:textAlignment="gravity"
                android:textColor="@color/Black"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textViewDay"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@+id/imageViewTeams"
                android:layout_toLeftOf="@+id/imageViewTeams2"
                android:layout_toRightOf="@+id/imageViewTeams"
                android:layout_toStartOf="@+id/imageViewTeams2"
                android:gravity="center"
                android:text="Friends"
                android:textAlignment="gravity"
                android:textColor="@color/Black"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textViewHour"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_alignLeft="@+id/textViewDay"
                android:layout_alignStart="@+id/textViewDay"
                android:layout_below="@+id/textViewDay"
                android:layout_toLeftOf="@+id/textViewTeamsName2"
                android:layout_toStartOf="@+id/textViewTeamsName2"
                android:gravity="center"
                android:text="Friends"
                android:textAlignment="gravity"
                android:textColor="@color/Black"
                android:textSize="15dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textViewTeamsName"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_marginLeft="12dp"
                android:gravity="center"
                android:text="Friends"
                android:textAlignment="gravity"
                android:textColor="@color/Black"

                android:textSize="12dp"
                android:textStyle="bold"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/imageViewTeams"
                android:layout_alignStart="@+id/imageViewTeams"
                android:layout_marginStart="12dp"
                android:layout_below="@+id/imageViewTeams2" />

            <TextView
                android:id="@+id/textViewTeamsName2"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_alignParentBottom="true"
                android:layout_below="@+id/imageViewTeams2"

                android:gravity="center"
                android:text="Friends"
                android:textAlignment="gravity"

                android:textColor="@color/Black"
                android:textSize="12dp"
                android:textStyle="bold"
                android:layout_alignLeft="@+id/imageViewTeams2"
                android:layout_alignStart="@+id/imageViewTeams2" />


            <ImageView
                android:id="@+id/imageViewTeams"
                android:layout_width="80dp"
                android:layout_height="35dp"
                android:layout_marginBottom="25dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="25dp"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"
                card_view:srcCompat="@drawable/myprofile" />

            <ImageView
                android:id="@+id/imageViewTeams2"
                android:layout_width="80dp"
                android:layout_height="35dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/imageViewTeams"
                android:layout_marginEnd="15dp"
                android:layout_marginRight="15dp"
                android:adjustViewBounds="true"
                android:scaleType="fitXY"
                card_view:srcCompat="@drawable/myprofile" />

            <TextView
                android:id="@+id/textviewweek"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_above="@+id/imageViewTeams2"
                android:layout_toEndOf="@+id/textViewDate"
                android:layout_toRightOf="@+id/textViewDate"
                android:fontFamily="serif-monospace"
                android:gravity="center"
                android:text="Hafta"
                android:textColor="@color/teal"
                android:textSize="18dp"
                android:textStyle="bold" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>




</RelativeLayout>